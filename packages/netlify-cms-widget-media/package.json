{
  "name": "netlify-cms-widget-media",
  "description": "Widget for uploading images in Netlify CMS.",
  "version": "2.6.5",
  "homepage": "https://www.netlifycms.org/docs/widgets/#media",
  "repository": "https://github.com/netlify/netlify-cms/tree/master/packages/netlify-cms-widget-media",
