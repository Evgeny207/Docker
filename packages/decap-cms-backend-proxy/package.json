{
  "name": "decap-cms-backend-proxy",
  "description": "Proxy backend for Decap CMS",
  "version": "1.3.0-beta.0",
  "repository": "https://github.com/decaporg/decap-cms/tree/master/packages/decap-cms-backend-proxy",
  "bugs": "https://github.com/decaporg/decap-cms/issues",
  "license": "MIT",
  "module": "dist/esm/index.js",
  "main": "dist/decap-cms-backend-proxy.js",
  "keywords": [
    "decap-cms",
    "backend"
  ],
  "sideEffects": false,
  "scripts": {
    "develop": "yarn build:esm --watch",
    "build": "cross-env NODE_ENV=production webpack",
    "build:esm": "cross-env NODE_ENV=esm babel src --out-dir dist/esm --ignore \"**/__tests__\" --root-mode upward --extensions \".js,.jsx,.ts,.tsx\""
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "decap-cms-lib-util": "^2.12.3",
    "decap-cms-ui-default": "^2.12.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4 || ^17.0.0"
  }
}
